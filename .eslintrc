{
  /* See all the pre-defined configs here: https://www.npmjs.com/package/eslint-config-defaults */
  "extends": "defaults/configurations/eslint",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "amd": true,
    "browser": true,
    "jquery": true,
    "node": true,
    "es6": true,
    "worker": true
  },
    "plugins": [
      "security"
  ],
    "extends": [
      "plugin:security/recommended"
  ],
  "rules": {
    "eqeqeq": 2,
    "comma-dangle": 1,
    "no-console": 0,
    "no-debugger": 1,
    "no-extra-semi": 1,
    "no-extra-parens": 1,
    "no-irregular-whitespace": 1,
    "no-undef": 0,
    "no-unused-vars": 1,
    "no-var": 1,
    "quotes": [
      2, "single",
      "avoid-escape"
    ],
    "semi": 1,
    "semi-spacing": 1,
    "valid-jsdoc": [
      1,
      {
        "prefer": {
          "arg": "param",
          "argument": "param",
          "class": "constructor",
          "return": "return",
          "virtual": "abstract"
        },
        "requireReturn": false
      }
    ]
  }
}
